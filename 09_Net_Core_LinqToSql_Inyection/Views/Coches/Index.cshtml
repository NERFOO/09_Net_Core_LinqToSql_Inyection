@model ICoche

<h1>Coches -- Inyeccion de dependencias</h1>

<h2>@Model.Marca</h2>
<h2>@Model.Modelo</h2>

<img src="~/images/@Model.Imagen" style="width: 300px; height: 200px"/>

<h2 style="color: green">@Model.Velocidad</h2>

<h2 style="color: red">@Model.VelocidadMaxima</h2>

<form method="post">
    <button name="accion" value="acelerar" class="btn btn-success">Acelerar</button>

    <button name="accion" value="frenar" class="btn btn-danger">Frenar</button>
</form>








