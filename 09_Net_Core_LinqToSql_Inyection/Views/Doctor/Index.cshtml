@model List<Doctor>


<h1>Doctores</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Hospital Cod</th>
            <th>Num Doctor</th>
            <th>Apellido</th>
            <th>Especialidad</th>
            <th>Saalrio</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Doctor doctor in Model)
        {
            <tr>
                <td>@doctor.HospitalCod</td>
                <td>@doctor.DoctorNum</td>
                <td>@doctor.Apellido</td>
                <td>@doctor.Especialidad</td>
                <td>@doctor.Salario</td>
            </tr>
        }
    </tbody>
</table>